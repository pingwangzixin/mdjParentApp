/*公共资源中心--全部资源================================================================*/
/*头部-----------------------*/
body{background:#eae9e9;}
.headFix{position:fixed;left:0;top:0;width:100%;z-index:11;background:#fff;}
.allHeadWrap{background:#fff;font-size:0.3rem;color:#666;line-height:0.8rem;}
.allHeadWrap div{float:left;width:33.333%;text-align:center;}
.allHeadWrap div span{display:inline-block;}
.allHeadWrap div span.active{color:#24bfff;border-bottom:0.03rem solid #24bfff;}

/*资源格式 作者------------*/
.listWrap{background:#f7f7f7;width:100%;overflow:hidden;}
.listWrap>ul{width:900%;}
.listWrap .line{height:1px;background:#f1f1f1;}
.listWrap .format{transition:transform 0.3s;}
.format .swipeLeft{display:inline-block;}
.author>div,.format .swipeLeft>div,.author>span,.format span{color:#333;display:inline-block;}
.author>div,.format .swipeLeft>div{line-height:0.75rem;font-size:0.28rem;padding:0 0 0 0.2rem;}
.author>span,.format span{padding:0 0.36rem;font-size:0.26rem;line-height:0.75rem;}
.author>span.active,.format span.active{color:#24bfff;}
/*下载量--------------------*/
.loadDetailWrap{background:#eae9e9;padding:0 0.2rem;overflow:hidden;}
.loadDetailWrap>div.fl{line-height:0.7rem;font-size:0.24rem;}
.loadDetailWrap>div.fl>span{color:#ff4864;}
.loadDetailWrap .uploadNum,.loadDetailWrap .clickNum{padding-right:0.2rem;background:url(../img/up-icon.png) no-repeat right center;background-size:0.14rem 0.21rem;font-size:0.24rem;line-height:0.7rem;color:#333;}
.clickNum.active{background-image:url(../img/down-icon.png);}
.loadDetailWrap .uploadNum{margin-right:0.34rem;background-image:url(../img/down-icon.png);}
.loadDetailWrap .uploadNum.active{background-image:url(../img/up-icon.png);}
.loadDetailWrap .voteNum{margin-left:0.34rem;}
/*列表展示------------------*/
.item_listWrap ul{background:#eae9e9;overflow:hidden;}
.item_listWrap li{background:#fff;margin-bottom:0.2rem;position:relative;}
.item_listWrap .listTitle{line-height:0.7rem;padding:0 0.2rem;font-size:0.28rem;color:#333;border-bottom:1px solid #ddd;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;box-sizing:border-box;}
.item_list{padding:0.2rem 0.15rem 0.2rem 0.2rem;}
.item_list .itemLeft{width:1.15rem;height:0.97rem;overflow:hidden;}
.item_list .itemLeft img{height:0.97rem;margin:0 auto;}
.item_list .itemCenter{margin-left:0.2rem;width:3.28rem;}
.item_list .itemRight{position:relative;z-index:4;}
.item_list .itemTxt{width:3.28rem;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;font-size:0.24rem;color:#666;line-height:0.24rem;padding:0.03rem 0 0.13rem 0;}
.item_list .visitNumWrap{width:4rem;margin-top:-0.04rem;height: 0.28rem; overflow: hidden;text-overflow: ellipsis;white-space: nowrap;font-size: 0.24rem;line-height: 0.24rem;}
.item_list .visitNumWrap span{float:left;font-size:0.24rem;line-height:0.28rem;margin-right:0.26rem;background:url(../img/look-icon.png) no-repeat left center;}
.item_list .visitNumWrap  .visitNum{color:#43c443;padding-left:0.38rem;background-image:url(../img/look-icon.png) ;background-size:0.32rem 0.21rem;}
.item_list .visitNumWrap  .starNum{color:#fca800;padding-left:0.28rem;background-image:url(../img/collect-icon.png);background-size:0.24rem;}
.item_list .visitNumWrap  .vtNum{color:#ff4864;padding-left:0.32rem;background-image:url(../img/vote-icon.png);background-size:0.27rem 0.23rem;}
.itemRight .itemR_text{width:2.41rem;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;font-size:0.24rem;color:#666;line-height:0.24rem;padding:0.03rem 0 0.13rem 0;}
/*精品图标样式-----------------*/
.boutique{position:absolute;right:0.82rem;top:0.74rem;width:1.13rem;height:1.1rem;z-index:3;}
/*搜索页面===========================================================================*/
.fold{background:#eae9e9;text-align:center;}
.fold>span{font-size:0.28rem;color:#43c443;line-height:0.7rem;padding:0 0.28rem 0 0;background:url(../img/takeback.png) no-repeat right center;background-size:0.18rem;margin:0 auto;display:inline-block;}
/*搜索-----------------------*/
.searchWrap{padding:0.25rem 0.2rem;background:#fff;margin-bottom:0.2rem;}
.searchWrap .searchTitle{padding-bottom:0.13rem;color:#333;font-size:0.28rem;line-height:0.28rem;}
.searchWrap .searchInp,.searchWrap .searchTimeWrap div{box-sizing:border-box;border:1px solid #d3d3d3;height:0.72rem;padding-left:0.1rem;font-size:0.28rem;line-height:0.7rem;border-radius:0.05rem;}
.searchWrap .searchInp{width:100%;margin-bottom:0.35rem;color:#333;}
.searchWrap .searchTimeWrap{width:100%;height:0.72rem;}
.searchWrap .searchTimeWrap div{width:2.82rem;float:left;background:url(../img/calendar-icon.png) no-repeat 2.43rem center;padding-right:0.41rem;background-size:0.31rem 0.35rem;color:#666;}
.searchWrap .searchTimeWrap div#endTime{margin-left:0.34rem;}
.searchWrap .searchTimeWrap span{float:right;width:0.78rem;height:0.78rem;background:#24bfff url(../img/search-icon.png) no-repeat center;background-size:cover;border-radius:50%;margin-top:-0.03rem;}
/*搜索无结果-----------*/
.noResultWrap{height:6.86rem;width:100%;background:#fff;padding-top:0.58rem;display:none;}
.noResultImg{width:2.85rem;height:4.56rem;margin:0 auto;background:url(../img/no_result.png) no-repeat center;background-size:2.85rem 4.56rem;}
.noResultTxt{padding:0.48rem 0 0.2rem 0;text-align:center;font-size:0.24rem;color:#666;line-height:0.24rem;}
.noResultBack{width:4.18rem;border-radius:0.05rem;margin:0 auto;display:block;line-height:0.6rem;font-size:0.3rem;color:#fff;background:#24bfff;text-align:center;}

/*加载样式-------------*/
.bottomFixWrap{position:fixed;bottom:0;left:0;width:100%;height:0.84rem;z-index:20;}
.bottomFixWrap .fixed{width:100%;height:0.84rem;}
.bottomFixWrap .fixedLeft,.bottomFixWrap .fixedRight{float:left;background:#878787;text-align:center;padding:0 0.12rem;box-sizing:border-box;height:0.84rem;}
.bottomFixWrap .fixedLeft.active,.bottomFixWrap .fixedRight.active{background:#f5f5f5;border-right:0 none;}
.bottomFixWrap .fixedLeft.active .nianjiWrap,.bottomFixWrap .fixedRight.active .nianjiWrap{color:#333;}
.bottomFixWrap .fixedLeft.active .nianjiWrap,.bottomFixWrap .fixedRight.active .nianjiWrap{background-image:url(../img/dropdown_black.png);}
.bottomFixWrap .fixedLeft{width:42.4%;border-right:1px solid #b6b6b6;}
.bottomFixWrap .nianjiWrap{font-size:0.24rem;color:#fff;max-width:2.9rem;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;line-height:0.84rem;padding-right:0.26rem;background:url(../img/dropdown_white.png) no-repeat right center;background-size:0.2rem 0.16rem;margin:0 auto;}
.bottomFixWrap .fixedRight{width:57.6%;}
.bottomFixWrap .fixedRight .nianjiWrap{max-width:3.9rem;}
/*底部点击显示层--------*/
.fixedLWrap{position:absolute;left:0;bottom:0.84rem;height:6.26rem;width:100%;background:#f5f5f5;}
.fixedLWrap .fixedTitle{line-height:0.6rem;font-size:0.24rem;color:#333;padding-left:0.18rem;}
.fixedLWrap .xdWrap{height:5.68rem;width:100%;position:relative;}
.fixedLWrap .xdWrap .nj{width:1.36rem;position:absolute;left:0;top:0;height:5.68rem;overflow-y:auto;}
.fixedLWrap .xdWrap .nj li{width:1.36rem;text-align:center;line-height:0.5rem;font-size:0.26rem;color:#666;margin-bottom:0.1rem;}
.fixedLWrap .xdWrap .nj li.active{background:#fff;color:#333;}
.fixedLWrap .subjectWrap{margin-left:1.36rem;height:5.68rem;overflow-y:auto;}
.fixedLWrap .subjectWrap li{background:#fff;padding:0 0.2rem;}
.fixedLWrap .subjectWrap .subj{height:0.86rem;}
.fixedLWrap .subjectWrap li.active .ver{padding:0;height:0;border:0 none;transition:padding,height,border 0.3s;}
.fixedLWrap .subjectWrap li.active .foldArr{transform:rotateX(180deg);-webkit-transform:rotateX(180deg);}
.fixedLWrap .subjectWrap .sub{font-size:0.26rem;/* height:0.26rem; */border-left:0.05rem solid #24bfff;margin:0.3rem 0 0.3rem 0;padding-left:0.1rem;width:4.7rem;overflow:hidden;white-space:nowrap;text-overflow:ellipsis;color:#333;line-height:0.26rem;}
.fixedLWrap .subjectWrap .foldArr{padding:0 0.5rem;margin-right:-0.2rem;height:0.86rem;background:url(../img/drop-down-gray.png) no-repeat center center;background-size:0.32rem 0.18rem;}
.fixedLWrap .subjectWrap .ver{overflow:hidden;padding-bottom:0.12rem;}
.fixedLWrap .subjectWrap .ver{padding-bottom:0.04rem;border-bottom:1px solid #ccc;padding-left:0.02rem;}
.fixedLWrap .subjectWrap .ver>span{margin:0.12rem 0 0.2rem -0.02rem;padding:0 0.48rem;color:#333;font-size:0.26rem;line-height:0.26rem;border:0.02rem solid #ccc;border-top:0 none;border-bottom:0 none;float:left;}
.fixedLWrap .subjectWrap .ver>span.active{color:#24bfff;}
/*zhezhao--------*/
.zhezhao{z-index:10;width:100%;height:100%;background:rgba(0,0,0,.3);position:absolute;left:0;top:0;z-index:3;}

.allNoresult>img{margin:2.20rem auto 0;width:3.47rem;display:block;}
.allNoresult>p{padding-top:0.42rem;text-align:center;font-size:0.3rem;line-height:0.3rem;color:#999;}
