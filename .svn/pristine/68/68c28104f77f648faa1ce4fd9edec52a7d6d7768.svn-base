<script type="text/javascript">
	//默认初始化--不需要修改
		mui.init({
		  pullRefresh : {
		    container:"#refreshContainer",//下拉刷新容器标识，querySelector能定位的css选择器均可，比如：id、.class等
		    down : {
		      height:50,//可选,默认50.触发下拉刷新拖动距离,
		      auto: false,//可选,默认false.自动下拉刷新一次
		      contentdown : "下拉可以刷新",//可选，在下拉可刷新状态时，下拉刷新控件上显示的标题内容
		      contentover : "释放立即刷新",//可选，在释放可刷新状态时，下拉刷新控件上显示的标题内容
		      contentrefresh : "正在刷新...",//可选，正在刷新状态时，下拉刷新控件上显示的标题内容
		      callback :pullfresh//必选，刷新函数，根据具体业务来编写，比如通过ajax从服务器获取新数据；
		    },
		    up : {
		      height:50,//可选.默认50.触发上拉加载拖动距离
		      auto:false,//可选,默认false.自动上拉加载一次
		      contentrefresh : "正在加载...",//可选，正在加载状态时，上拉加载控件上显示的标题内容
		      contentnomore:'没有更多数据了',//可选，请求完毕若没有更多数据时显示的提醒内容；
		      callback :pullfresh2 //必选，刷新函数，根据具体业务来编写，比如通过ajax从服务器获取新数据；
		    }
		  } 
		});
		
		//上拉加载
		var count = 0
		function pullfresh2() {
		    //业务逻辑代码，比如通过ajax从服务器获取新数据；
		    setTimeout(function() {
		     	mui('#refreshContainer').pullRefresh().endPullupToRefresh(true);
			     //注意：
			     //1、加载完新数据后，必须执行如下代码，true表示没有更多数据了：
			     //2、若为ajax请求，则需将如下代码放置在处理完ajax响应数据之后
		     },1000)
		}
		
		//下拉刷新
		function pullfresh() {
	     //业务逻辑代码，比如通过ajax从服务器获取新数据；
	     //注意，加载完新数据后，必须执行如下代码，注意：若为ajax请求，则需将如下代码放置在处理完ajax响应数据之后\n
	     setTimeout(function(){
		     mui('#refreshContainer').pullRefresh().endPulldownToRefresh();
	     },1000 )
	
		};
</script>
<script type="text/javascript">
        //调用手机日历插件
        window.onload=function(){

            (function(mui) {
                mui.init();
                $(document).on("tap","#startTime",function(){
                    var getTime= document.querySelector('#endTime').innerText;
                    var endToArr=getTime.replace(/[\u4E00-\u9FA5]+/g,"-").replace(/-$/,'').split('-');
                    var endArr=endToArr;
                    var year=Number(endArr[0]);
                    var month=Number((endArr[1]-1));
                    var day=Number(endArr[2]);
                    var dateTime=(endToArr.length!==1)?new Date(year,month,day):new Date();
                    var picker = new mui.DtPicker({"type": "date","endDate": dateTime}) ;
                    picker.show(function(rs){
                        if(endToArr.length===1){
                            document.querySelector("#startTime").innerText = rs.y.value+'年'+rs.m.value+'月'+rs.d.value+'日';
                        }else{
                            var dateStart=new Date(endToArr[0],(endToArr[1]-1),endToArr[2]);
                            var dateEnd=new Date(rs.y.value,(rs.m.value-1),rs.d.value);
                            if(dateStart<dateEnd){
                                document.querySelector("#startTime").innerText='不合理时间';
                                $("#startTime").css('color','red');
                            }else{
                                document.querySelector("#startTime").innerText = rs.y.value+'年'+rs.m.value+'月'+rs.d.value+'日';
                                $("#startTime").css('color','#666');
                            }
                        }
                        picker.dispose();
                    });
                });
                $(document).on("tap","#endTime",function(){
                    var getTime= document.querySelector('#startTime').innerText;
                    var endToArr=getTime.replace(/[\u4E00-\u9FA5]+/g,"-").replace(/-$/,'').split('-');
                    var endArr=(endToArr.length!==1)?endToArr:[2006,1,1];
                    var year=Number(endArr[0]);
                    var month=Number((endArr[1]-1));
                    var day=Number(endArr[2]);
                  
                    var picker = new mui.DtPicker({"type": "date","beginDate":new Date(year,month,day),"endDate":new Date()}) 
                    picker.show(function(rs){
                        if(endToArr.length===1){
                            document.querySelector("#endTime").innerText = rs.y.value+'年'+rs.m.value+'月'+rs.d.value+'日';
                        }else{
                            var dateStart=new Date(endToArr[0],(endToArr[1]-1),endToArr[2]);
                            var dateEnd=new Date(rs.y.value,(rs.m.value-1),rs.d.value);
                            if(dateStart>dateEnd){
                                document.querySelector("#endTime").innerText='不合理时间';
                                $("#endTime").css('color','red');
                            }else{
                                document.querySelector("#endTime").innerText = rs.y.value+'年'+rs.m.value+'月'+rs.d.value+'日';
                                $("#endTime").css('color','#666');
                            }
                        }
                        picker.dispose();
                    });
                });
            })(mui);
        }


$(function(){  
 var userAgent = navigator.userAgent;  
 alert(userAgent)  
 var index = userAgent.indexOf("Android")  
 if(index >= 0){  
   var androidVersion = parseFloat(userAgent.slice(index+8));   
   if(androidVersion<4.4){  
    // 版本小于3的事情  
    alert(androidVersion+'版本小于4');  
   }else{
   	alert(androidVersion+'版本>4');
   }
 }  
});  
</script>

