	 //判断Android  监听软键盘出现盒子位置变化
	if (/(Android)/i.test(navigator.userAgent)) {
		var h=$(window).height();
		$(window).resize(function(){
			if($(window).height()<h){
				$('.bottomFixWrap .fixed').hide();
				
			}else if($(window).height()===h){
				$('.bottomFixWrap .fixed').show();
			}
		})
	} 
	
