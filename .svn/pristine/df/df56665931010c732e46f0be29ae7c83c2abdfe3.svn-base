/*文件上传模块*/
body{background-color: #fff; position: relative;height: 100%;-webkit-overflow-scrolling: touch}
html{height: 100%;-webkit-overflow-scrolling: touch}
/*上半部分*/
.upload_head{background:white;  padding:0.3rem 0.25rem 0.6rem 0.2rem; position:relative; border-color:white}
.upload_input{height:0.72rem; line-height:0.72rem; margin-bottom:0.6rem}
.upload_input span{font-family: ''; color:#fb2a26; padding:0 0.12rem 0 0}
.upload_input input{height:0.72rem; border:1px solid #cecece; width:5.3rem; border-radius: 0.1rem; font-size:0.26rem; padding-left:0.15rem}
.upload_input input::-webkit-input-placeholder{color:#999999; font-family: "微软雅黑";}
.upload_input label{font-size:0.27rem; display: inline-block;}
.upload_input p .note{position:absolute; bottom:-0.3rem; left:0; line-height:1; font-size:0.22rem; color:#fe4a65; white-space: nowrap; }
.upload_input p{float:right; position: relative;}
.upload_input p textarea{width:5.3rem; height:1.8rem; border:1px solid #cecece; border-radius: 0.1rem; font-size:0.26rem; padding-left:0.15rem; padding-top:0.15rem; color:#000; font-family: "微软雅黑"; resize:none; overflow:auto}
.upload_head .upload_input:nth-of-type(4) p{height:1.95rem}
.upload_head .upload_input:nth-of-type(4){height:auto;  margin-bottom:0}
.upload_head .upload_input .change_dom{ width: 5.35rem; font-size:0.27rem}
.upload_head .upload_input .change_dom span{float: left;font-family:"微软雅黑";font-weight: 500; color:#999999;width: 33%; text-align:center;}
.upload_head .upload_input .change_dom span.active{color: #24bfff;}
.upload_head .upload_input:nth-of-type(2){margin-bottom: 0.3rem;}
/*center部分*/
.upload_choose{padding:0.37rem 0.25rem 0.23rem 0.2rem; background:#eee; border-color:#eee; position:relative;}
.upload_choose .upload_input input{background:#eee; color:#000; font-family: "微软雅黑"; font-size:0.24rem;}
.upload_choose .upload_input{margin-bottom:0.36rem; }
.upload_choose .upload_input:last-of-type label{word-break:break-word; line-height:1.4; width:1.25rem; max-width: 1.3rem;}
.upload_choose .upload_input:last-of-type{line-height:1.2}
.upload_choose .upload_input span{float:left}
.upload_head .border_jiantou{width:0;height:0; border-color:inherit; border-style: solid; border-width: 0.2rem; border-bottom:0; border-left-color:transparent; border-right-color:transparent; position:absolute; bottom:-0.18rem; left:50%; -webkit-transform: translateX(-50%);  z-index: 1;}
.upload_choose:after{width:0;height:0; border-color:inherit; border-style: solid; border-width: 0.2rem; border-bottom:0; border-left-color:transparent; border-right-color:transparent; position:absolute; bottom:-0.18rem; left:50%; -webkit-transform: translateX(-50%); content: ''; display:block; z-index: 1;}
/*文件上传*/
.upload_file {padding:0.36rem 0 0.3rem; position:relative;}
.upload_file .filewrap{text-align: center;}
.upload_file .filewrap img{width:1.45rem; height:1.45rem;}
.upload_file p{text-align:center; color:#63cdff;}
.upload_file .showfile{font-size:0.2rem;display:block; height:0.45rem;line-height:0.25rem; padding:0.2rem 0 0 0.4rem; float:left; margin-left:50%; -webkit-transform: translateX(-50%); border-radius: 1.5rem; background-color:#fda800; padding-right:0.75rem; position:relative; color:white; max-width: 70%; } 
.upload_file .showfile>span{float:left; white-space: nowrap ; text-overflow: ellipsis; overflow: hidden; width:100%}
.upload_file .showfile img{position:absolute; right:0.2rem; margin-top:-0.1rem; margin-left:0.2rem; width:0.44rem; height:0.44rem; }
.upload_file .showgress{font-size:0.27rem; height:0.67rem; line-height: 0.67rem;  border-radius: 1.5rem;background-color:#fda800;width:5rem; margin:0 auto; text-align:center }
.upload_file .showgress>span{ white-space: nowrap; color:white;  }
.upload_file input[type=file]{position:absolute; width:1.5rem; height:1.8rem;  left:3rem; opacity:0; z-index: 20; }
/*文件上传描述*/
.filenote{padding-left:0.2rem; font-size:0.22rem; line-height:0.4rem; padding-bottom:0.16rem; color:#666}
#upload_sure{height:0.7rem; background:#ebebeb; text-align: center; line-height: 0.7rem; color:#686767; font-size:0.28rem;}

/*弹层*/
.upload_popup{position:absolute; width:100%; height:100%; top:0}
.del_down .upload_del{width:5.92rem; height:2.51rem; background:white; border-radius: 0.12rem;}
.del_down .upload_del p{line-height:1.44rem; font-size:0.28rem; color:#000}
.del_down .upload_del button{width:1.38rem; height:0.62rem; background:#24bfff; color:white; font-family: "微软雅黑"; font-size:0.26rem; border-radius:0.12rem}
.del_down .upload_del button:nth-of-type(2){margin-left:0.45rem; background:#d9d9d9}
.del_down .upload_del button:active{background:pink}
.del_down .message{width:5.92rem; height:2.5rem; background:white; border-radius: 0.12rem; text-align: center; line-height: 2.5rem; position: fixed; top: 30%;left: 50% ;  -webkit-transform: translate(-50% , -50% );   -ms-transform: translate(-50% , -50% ); transform: translate(-50% , -50% ); z-index:103; border-radius: 5px; opacity: 1; -webkit-box-shadow: 0 0 6px #ccc; box-shadow: 0 0 6px #ccc;}
.del_down .uploadEnd{width:5.92rem; height:2.5rem; background:white; border-radius: 0.12rem; text-align: center; line-height: 2.5rem; position: fixed; top: 30%;left: 50% ;  -webkit-transform: translate(-50% , -50% );   -ms-transform: translate(-50% , -50% ); transform: translate(-50% , -50% ); z-index:103; border-radius: 5px; opacity: 1; -webkit-box-shadow: 0 0 6px #ccc; box-shadow: 0 0 6px #ccc; font-size:0.36rem}
.del_down .con{position:fixed}
/*选择年级*/
/*.upload_popup .foot_note li {float:left;  margin-right:0.56rem; font-size:0.24rem; margin-bottom:0.54rem; margin-top:0.2rem}*/
.upload_popup .foot_note li{float:left; width:1.08rem;  margin-top:0.2rem; font-size:0.24rem; text-align: left; height: 0.8rem; ; overflow: hidden; /* text-overflow: ellipsis;*/ display: -webkit-box;  -webkit-box-orient: vertical; -webkit-line-clamp: 2; line-height: 0.4rem; padding-right:0.2rem ; }
.upload_popup .foot_note li:nth-of-type(6n){width:0.8rem; padding-right:0.2rem}
.upload_popup .foot_note{position:fixed; width:100%; z-index: 102; padding:0.28rem 0.2rem 0;  bottom:0; left:0; overflow:hidden;background:white;}
.upload_popup .foot_note ul{width:100%; overflow-y: scroll;  max-height: 5.6rem; }
.upload_popup .foot_note li.active{color:#25bfff}
/*选择单元*/
.upload_popup .foot_unit{position: fixed; z-index: 103; left:0; bottom:0; width:100%; max-height:7rem; text-align: left; background:white; overflow-y: scroll;}
.upload_popup .foot_unit li{height:1.05rem; border-bottom: 1px solid #ccc; padding-left:0.34rem; line-height: 1.05rem;}
.upload_popup .foot_unit li span{margin-right:0.1rem}
.upload_popup .foot_unit li.active{color:#25bfff}





/*捷成影视模块*/
.movieNav{height:0.8rem;line-height:0.8rem ; /*display: flex;*/ }
.movieNav li{float:left;width:33.33%;  text-align: center; overflow: hidden; height:100%}
.movieNav li span{ display:inline-block; height:100%; -webkit-box-sizing:border-box;}
.movieNav li span.active{ border-bottom:3px solid #24bfff; color:#24bfff}
.moviewSearch {height:0.6rem; background:#eeeeee; padding:0.2rem; }
.moviewSearch .serachwrap{ background:white; height:100%; border-radius: 1rem; padding:0 0.24rem; padding-right:0}
.moviewSearch .serachwrap input{height:100%; font-size:0.24rem; float:left; width:5.5rem; padding-left:0.1rem}
.moviewSearch .serachwrap input::-webkit-input-placeholder{color:#999999; font-family: "微软雅黑";}
.moviewSearch .serachwrap button{float:right; padding:0 0.24rem; line-height: 0.4rem;  padding-top:0.16rem; background:none}
.moviewSearch .serachwrap button img{width:0.32rem; height:0.32rem; }
.movies li{margin-left:0.19rem; margin-top:0.2rem; float:left; font-size:0.25rem;}
.movies li img{width:2.25rem; height:3.05rem; }

/*捷成影视搜索部分*/
.searchTXT{height:0.68rem; background: #eee; line-height:0.68rem; padding-left: 0.2rem; }
.searchTXT span{padding-left:0.22rem; color:#24bfff; }
.movieRes li{padding:0.3rem 0.2rem; border-bottom:1px solid #ddd; margin-right:0.4rem}
.movieRes li img{float:left; margin-right:0.2rem; width:2.1rem; height:2.84rem; }
.movieRes li .movieCon h3{font-size:0.27rem; line-height:0.4rem; }
.movieRes li .movieCon p{line-height:0.36rem;}
.movieRes li .movieCon i{float:left; margin-top:0.78rem; background:#ffa800; width:1.1rem; height:0.6rem; border-radius: 0.1rem; line-height:0.6rem; padding-left: 0.5rem; background-image: url(../img/moviekan.png); background-repeat: no-repeat; background-position: 0.2rem ; background-size: 0.18rem 0.22rem; color:white}
.searchInpWrap{padding:0.24rem 0.2rem 0.24rem; }
.searchInpWrap .searchInp{background:#ebebeb; padding-left:0.1rem;  border-radius: 0.11rem;  float:right}
.searchInpWrap span{float:left; line-height: 0.69rem; }
.searchInp input{float:left; width:5.3rem; height:0.69rem; background:#EBEBEB; padding-left:0.15rem; font-size: 0.26rem; font-family:'microsoft yahei'; color:#999}
.searchInp button{float:right; height:0.69rem; padding-left:0.3rem; background:none; padding-right:0.3rem;}
.searchInp button img{width:0.3rem; height:0.3rem}
.searchInp input::-webkit-input-placeholder{color:#bbb; font-size: 0.26rem; font-family:'microsoft yahei'; }

