.del_down .con3{
    width:90%;
    height:8rem;
    background-color: #fff;
    position: fixed;
    top: 50%;
    left: 50% ; 
    -webkit-transform: translate(-50% , -50% ); 
    -ms-transform: translate(-50% , -50% );
    transform: translate(-50% , -50% );
    z-index:103;
    border-radius: 5px;
    opacity: 1;
    -webkit-box-shadow: 0 0 6px #ccc;
    -moz-box-shadow: 0 0 6px #ccc;
    box-shadow: 0 0 6px #ccc;
}
