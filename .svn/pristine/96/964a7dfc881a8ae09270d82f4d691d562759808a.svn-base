/*底部固定*/		
.fixed_bottom{width:100%;height: 0.84rem;position: fixed;bottom: 0;background-color: #878787;overflow: hidden;color:#fff;}
.fixed_bottom .sub{width: 100%;text-align: center;}
.fixed_bottom span{
	display: inline-block;
	height: 0.84rem;
	line-height: 0.84rem;
	text-align: center;
	font-size: 0.25rem;
}
.fixed_bottom i{
	display: inline-block;
	width: 0.22rem;
    height: 0.2rem;
	margin: 0 0.03rem;
	background: url(../img/dropdown_white.png) no-repeat center;
	background-size: 100% 100%;
}
.fixed_box .con{
	width: 100%;
	height: 2.22rem;
	position: fixed;
	/*top:calc(100% - 3.06rem);*/ 
	top: auto;
	left:0;
	bottom: 0.84rem;
	-webkit-transform: translate(0% , 0% ); 
    -ms-transform: translate(0% , 0% );
    transform: translate(0% , 0% );
    border-radius: 0;
    text-align: left;
}
.fixed_box .con  span{
	display: inline-block;
	width: 15.5%;
	text-align: center;
    line-height: 1rem;

}
.fixed_box .con  span.active{color:#24bfff;}

.fixed_active{background-color: #fff;color:#333;}
.fixed_active i{background: url(../img/dropdown_black.png) no-repeat center;background-size: 100% 100%;}
.con1{position: fixed;left: 50%;top: 50%;margin-left: -0.3rem;}
.con2{position: fixed;left: 50%;top: 50%;margin-left: -1.5rem;z-index:1001;}
